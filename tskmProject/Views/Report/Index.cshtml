@using DotNet.Highcharts;
@{
    ViewBag.Title = "สถิติการปฏิบัติงาน";
}

<div class="row">
    <div class="col-md-12">
        <h3 class="page-header">สถิติการปฏิบัติงาน</h3>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Ticket Status Ratio
            </div>
            <div class="panel-body">
                @((Highcharts)ViewBag.TicketStatusRatioChart)
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                Opened/Closed Ticket in each month
            </div>
            <div class="panel-body">
                @((Highcharts)ViewBag.TicketOpenCloseStatChart)
            </div>
        </div>
    </div>
</div>

@section scripts {
    @Scripts.Render("~/bundles/highcharts")
}