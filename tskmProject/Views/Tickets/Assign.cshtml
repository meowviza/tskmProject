@model tskmProject.Models.Ticket

@{
    ViewBag.Title = "มอบหมาย";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="row">
        <h3 class="page-header">ใบงาน</h3>
    </div>
    <div class="panel panel-default">
        <div class="panel-body">
            <br />
            <div class="form-group">
                @Html.LabelFor(m => m.userID, "มอบหมาย", new { @class = "col-md-2 control-label" })
                <div class="col-md-5">
                    @Html.DropDownList("userID", null, new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.userID, "", new { @class = "text-danger" })
                </div>
            </div>

            <br />
                <div class="col-md-offset-2 col-md-10">
                    <button type="submit" value="Create" class="btn btn-default">ยืนยันข้อมูล</button>
                    @Html.ActionLink("กลับสู่หน้าหลัก", "Index")
                </div>


        </div>
    </div>

}


